    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "fastify-tsconfig": "^2.0.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.1.0",
    "msw": "^2.0.6",
    "prettier": "^3.1.0",
