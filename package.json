  "scripts": {
    "build": "node build.js",
    "dev": "pnpm build && concurrently -k -p \"[{name}]\" -n \"Typescript,App\" -c \"yellow.bold,cyan.bold\" \"pnpm watch:tsc\" \"pnpm dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -p 3003 -w -l info -P dist/app.js",
    "lint": "eslint . --ext .js,.ts && prettier . --check && tsc -p tsconfig.json --noEmit --incremental false",
    "lint:fix": "eslint . --ext .js,.ts --fix && prettier . --write && tsc -p tsconfig.json --noEmit --incremental false",
    "prepare": "is-ci || husky install",
    "start": "node dist/server.js",
    "test": "cross-env NODE_NO_WARNINGS=1 vitest",
    "watch:esbuild": "esbuild --watch --platform=node --target=es2022 --outdir=dist src/*.ts src/**/*.ts src/**/**/*.ts",
    "watch:tsc": "tsc -w -p tsconfig.json"
  },
  "dependencies": {
    "@buf/martines3000_proto-example.bufbuild_es": "1.4.2-20231107160053-20658d92a322.1",
